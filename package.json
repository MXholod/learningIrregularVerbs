{
  "name": "verbs",
  "version": "0.0.1",
  "description": "learning irregular verbs",
  "main": "http://localhost:3000",
  "node-main": "./server.js",
  "scripts": {
    "dist": "build --tasks win-x64 --mirror https://dl.nwjs.io/ .",
    "start": "run --x64 --mirror https://dl.nwjs.io/ .",
    "build": "webpack"
  },
  "author": "MX",
  "license": "ISC",
  "devDependencies": {
    "css-loader": "^2.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "node-sass": "^4.13.1",
    "nwjs-builder-phoenix": "^1.15.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "webpack": "3.11.0"
  },
  "window": {
    "title": "English Program",
    "position": "center",
    "width": 800,
    "height": 600,
    "min-height": 600,
    "min-width": 800,
    "max-height": 600,
    "max-width": 800,
    "resizable": false
  },
  "build": {
    "appId": "io.github.evshiron.nwjs-builder-phoenix.project",
    "nwVersion": "0.36.4",
    "packed": true,
    "targets": [
      "zip"
    ],
    "strippedProperties": [
      "build"
    ],
    "outputPattern": "${NAME} ${VERSION} ${PLATFORM} ${ARCH}",
    "win": {
      "productName": "Irregular verbs",
      "companyName": "Phoenix",
      "copyright": "copyright"
    }
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "bootstrap": "4.0.0",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "nedb": "1.8.0",
    "nodemailer": "4.6.7",
    "pug": "2.0.0-rc.4",
    "socket.io": "2.1.0"
  }
}

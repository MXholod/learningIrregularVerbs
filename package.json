{
  "name": "verbs",
  "version": "0.0.1",
  "description": "learning irregular verbs",
  "main": "http://localhost:3000",
  "node-main": "./server.js",
  "scripts": {
    "dist": "build --tasks win-x64 --mirror https://dl.nwjs.io/ .",
    "start": "run --x64 --mirror https://dl.nwjs.io/ .",
    "build": "webpack"
  },
  "author": "MX",
  "license": "ISC",
  "devDependencies": {
    "css-loader": "0.28.9",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "node-sass": "4.7.2",
    "nwjs-builder-phoenix": "1.14.6",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.1",
    "webpack": "3.11.0"
  },
  "window": {
    "title": "English Program",
    "position": "center",
    "width": 790,
    "height": 590,
    "min-height": 590,
    "min-width": 790,
    "max-height": 590,
    "max-width": 790,
    "resizable": false
  },
  "build": {
    "appId": "io.github.evshiron.nwjs-builder-phoenix.project",
    "nwVersion": "0.31.3",
    "packed": true,
    "targets": [
      "zip"
    ],
    "strippedProperties": [
      "build"
    ],
    "outputPattern": "${NAME} ${VERSION} ${PLATFORM} ${ARCH}",
    "win": {
      "productName": "Irregular verbs",
      "companyName": "Phoenix",
      "copyright": "copyright"
    }
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "bootstrap": "4.0.0",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "nedb": "1.8.0",
    "nodemailer": "4.6.7",
    "pug": "2.0.0-rc.4",
    "socket.io": "2.1.0"
  }
}
